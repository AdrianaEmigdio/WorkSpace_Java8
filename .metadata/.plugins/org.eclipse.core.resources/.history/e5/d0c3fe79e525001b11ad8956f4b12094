
public class Biblioteca {
public static void main(String[] args) {
	
	Livro l1, l2, l3 ;
	
	l1 = new Livro ("Java Basico", "Isidro", 2020,"Terror",1,2, false);
	l2 = new Livro ("Java 00"Isidro", 2020, "Ciencias Ocultas",3,4, false);"
	l3 = new Livro ("Java 00"Isidro", 2020, "Ciencias Ocultas",3,4, false);
		
	System.out.println(l1.imprimir());
	System.out.println(l2.imprimir());
	System.out.println(l3.imprimir());
	
	System.out.println("------------> Emprestando o Livro L1");
	l1.emprestar();
	System.out.println(l1.imprimir());
	
	
	System.out.println("------------> Devolvendo o Livro L1");
	l1.devolver();
	System.out.println(l1.imprimir());
}
}
